snippet ifmain "if __name__ == __main__:"
if __name__ == "__main__":
	${1:main()}
endsnippet


snippet Log "log config" 
import logging

LOG_FORMAT = "%(asctime)s - %(levelname)s - %(filename)s[%(lineno)d] - %(message)s"
DATE_FORMAT = "%m/%d/%Y %H:%M:%S %p"

logging.basicConfig(filename='${1:filename}', level=logging.DEBUG, format=LOG_FORMAT, datefmt=DATE_FORMAT)


endsnippet

